      " (rows, columns):  (655,)\n"
     ]
    }
   ],
   "source": [
    "#Enter Your Code, Execute and take the Screenshot\n",
    "x_train, x_test, y_train, y_test = train_test_split(features, Y, test_size=0.2, random_state=10)\n",
    "print('Train data set:', '\\n')\n",
    "print('Train X: ', '\\n', '(rows, columns): ', x_train.shape)\n",
    "print('Train Y: ', '\\n', '(rows, columns): ', y_train.shape)\n",
    "print('Test X: ', '\\n', '(rows, columns): ', x_test.shape)\n",
    "print('Test Y: ', '\\n', '(rows, columns): ', y_test.shape)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
